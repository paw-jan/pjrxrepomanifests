<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Remotes -->
  <remote
      name="yocto"
      fetch="https://git.yoctoproject.org/"
  />

  <remote
      name="github"
      fetch="https://github.com/"
  />

  <!-- Default settings -->
  <default
      revision="kirkstone"
      remote="yocto"
      sync-j="4"
  />

  <!-- Poky -->
  <project
      name="poky"
      path="poky"
      revision="kirkstone"
  />

  <!-- Raspberry Pi BSP -->
  <project
      name="agherzan/meta-raspberrypi"
      path="meta-raspberrypi"
      remote="github"
      revision="kirkstone"
  />

  <!-- OpenEmbedded Layers -->
  <project
      name="openembedded/meta-openembedded"
      path="meta-openembedded"
      remote="github"
      revision="kirkstone"
  />

  <!-- PJ RX RPi5 repo -->
  <project
      name="paw-jan/meta-pjrxrpi5"
      path="meta-pjrxrpi5"
      remote="github"
      revision="main"
  />

</manifest>

